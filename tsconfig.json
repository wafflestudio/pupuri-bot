{
  "compilerOptions": {
    "target": "es6",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "moduleResolution": "Bundler",
    "allowArbitraryExtensions": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "types": ["vitest/globals"]
  }
}
