{
  "compilerOptions": {
    "target": "ES2024",
    "module": "ES2022",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "noEmit": true,
    "moduleResolution": "Bundler",
    "allowArbitraryExtensions": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true
  }
}
