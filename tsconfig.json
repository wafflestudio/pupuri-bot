{
  "compilerOptions": {
    "allowArbitraryExtensions": true,
    "esModuleInterop": true,
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "noUncheckedIndexedAccess": true,
    "outDir": "./dist",
    "rootDir": ".",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2024"
  }
}
